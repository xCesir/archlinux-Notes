UwU() {
    if [[ $@ =~ "([sS][eE][nN][pP][aA][iI] [fF][uU][cC][kK] [mM][eE])$" ]]
    then up;
    elif [[ $@ =~ "([fF][uU][cC][kK] [mM][eE] [sS][oO][fF][tT] [dD][aA][dD][dD][yY])$" ]]
    then upnq;
    else
        sudo $@;
    fi

}
function I(){
    if [[ $@ == "(will be back)$" ]]
    then reboot;
    fi
}
function you(){
    if [[ $@ =~ "(died)$" ]];
    then shutdown -P +0;
    fi
}
function gut(){
    sed -E 's/\"/\\"/g' <<< $@ | sed -E 's/gut/git/g' | xargs git | sed -E 's/Git/Gut/g' | sed -E 's/GIT/GUT/g' | sed -E 's/[gG][iI][tT]/gut/g'
}

function gitgut(){
   sed -E 's/\"/\\"/g' <<< $@ | sed -E 's/gitgut/git/g' | xargs git | sed -E 's/Git/GitGut/g' | sed -E 's/GIT/GITGUT/g' | sed -E 's/[gG][iI][tT]/gitgut/g'
}